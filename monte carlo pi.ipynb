    "import random as r\n",
    "import math as m\n",
    "\n",
    "inside = 0\n",
    "total = 1000000\n",
    "\n",
    "for i in range(0, total):\n",
    "  x2 = r.random()**2\n",
    "  y2 = r.random()**2\n",
    "  if m.sqrt(x2 + y2) < 1.0:\n",
    "    inside += 1\n",
    "\n",
    "pi = (float(inside) / total) * 4\n",
    "print(pi)\n"
   ]
